	var type = "0";
	function qdType(qtype) {
		type = qtype;
		if (type == "9"){
			$("#searchKey").prop("placeholder","Search data on findata") ;
		}else {
			$("#searchKey").prop("placeholder","可搜索数据集、数据应用、数据分析工具、作者、项目名称、CSTR、DOI") ;
		}
	}

	function searchKeyWord() {
		let key = $("#searchKey").val();
		if (type == "9"){
			if (key === ""|| typeof(key) == "undefined" || key === " "){
				window.open("https://findata.cn/?source=NBSDC");
			}else {
				window.open("https://findata.cn/search?source=NBSDC&search=" + key);
			}
		}else {
			if(key.indexOf("CSTR:") !== -1 || key.indexOf("cstr:") !== -1){
				window.open("https://cstr.cn/" + key.trim());
			} else{
				if(type === "0"){
					window.location.href = '/general/dataSetHome?searchKey='+key;
				}else{
					window.location.href = '/general/dataSetHome?searchKey='+key+'&tpe='+type;
				}
			}
		}


	}

	function hotData() {
		$.ajax({
			type: "GET",
			url: "/api/general/v1/getDataLists",
			contentType: 'application/json',
			dataType: "json",
			async: false,
			success: function (data) {
				let list = data.list.list;
				$.each(list, function (index, item) {
					// 数据组装
					$("#getDataLists").append('<li><a target="_blank" href="/general/dataDetail?id='+item.id+'&type='+item.dataTypeId+'" title="'+item.name+'">'+item.name+'</a><span class="hot">'+item.viewingCount+'</span></li>');
				});
			},
			error: function () {

			}
		});
	}

	function removeDiv(){
		let obj = document.getElementById("tipsId");
		obj.parentNode.removeChild(obj);
	}


	function newsList() {
		let params = {
			"pageNum": 1,
			"pageSize": 3,
			"sortName": "updateTime",
		};
		$.ajax({
			type: "POST",
			url: "/api/general/news/selectNewsNoticePages",
			data: JSON.stringify(params),
			contentType: "application/json",
			dataType: "json",
			cache: false,
			async: false,
			success: function (data) {
				let result = data.result.list;
				if (result.length > 0) {
					$.each(result,function (index,item) {
						// 数据组装
						let href = "/general/news/newsNoticeView?id="+item.id;
						$("#getNewsLists").append(
								'   <li><a target="_blank" href="'+href+'" title="'+item.title+'">'+item.title+'</a><span class="time">'+item.updateTime.substr(0,10)+'</span></li>');
					});
				}
			},
			error: function (XMLHttpRequest, textStatus) {
				alert(XMLHttpRequest.responseJSON.message);
			}
		});
	}
	function lunboAboutNew() {
		$.ajax({
			type: "GET",
			url: "/api/general/selectNewsBanner",
			contentType:"application/json",
			dataType: "json",
			cache: false,
			async : false,
			success: function (data) {
				let result = data.result;
				if(result.length > 0){
					setValue(result);
				}
			},
			error:function (XMLHttpRequest, textStatus) {
				console.log("异常错误");
			}
		});

		function setValue(result) {
			let contentHtml = '';
			$.each(result,function (index,item) {
				contentHtml += '<div class="swiper-slide">';
				contentHtml += '    <a href="/general/news/newsNoticeView?id='+item.id+'" target="_blank">'+item.title+'</a>';
				contentHtml += '</div>'
			});
			$("#bannerNews").append(contentHtml);

			new Swiper('.inote', {
				loop: true,
				speed: 1500,
				direction: 'vertical',
				autoplay: {
					delay: 3000,
					disableOnInteraction: false,
				}
			})
		}
	}
</script>
